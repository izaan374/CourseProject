pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Good') {
            steps{
                echo'Good Night'
                snykSecurity(
          snykInstallation: 'snykk',
          snykTokenId: 'snyk@latest',
          // place other optional parameters here, for example:
          additionalArguments: '--all-projects --detection-depth=<DEPTH>'
                )
           }
        }       
    }
}

