pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
               bat "rmdir  /s /q CourseProject"
                bat "git clone https://github.com/izaan374/CourseProject.git"
                bat "mvn clean -f CourseProject"
            }
        }
        stage('install') {
            steps {
                bat "mvn install -f CourseProject"
            }
        }
        stage('test') {
            steps {
                bat "mvn test -f CourseProject"
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f CourseProject"
            }
        }
    }
}
